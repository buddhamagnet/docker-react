{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "multi-react",
      "image": "buddhamagnet/multi-react",
      "hostname": "client",
      "essential": false
    },
    {
      "name": "multi-api",
      "image": "buddhamagnet/multi-api",
      "hostname": "api",
      "essential": false
    },
    {
      "name": "multi-worker",
      "image": "buddhamagnet/multi-worker",
      "essential": false
    },
    {
      "name": "multi-facade",
      "image": "buddhamagnet/multi-facade",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["multi-react", "multi-api"]
    }
  ]
}
